{
  "name": "@dzangolab/pulumi",
  "version": "0.30.2",
  "description": "A collection of opinionated Pulumi component resources and dynamic providers",
  "keywords": [
    "pulumi",
    "AWS",
    "ECR"
  ],
  "homepage": "https://github.com/dzangolab/pulumi",
  "bugs": {
    "url": "https://github.com/dzangolab/pulumi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dzangolab/pulumi.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/dzangolab-pulumi.js",
      "require": "./dist/dzangolab-pulumi.umd.cjs"
    }
  },
  "main": "./dist/dzangolab-pulumi.umd.cjs",
  "module": "./dist/dzangolab-pulumi.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly && mv dist/src dist/types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "release": "shipjs prepare",
    "sort-package": "npx sort-package-json",
    "typecheck": "tsc --noEmit -p tsconfig.json --composite false"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@pulumi/random": "4.17.0",
    "globals": "^15.14.0",
    "google-protobuf": "3.21.4",
    "nunjucks": "3.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@eslint/compat": "^1.2.6",
    "@types/node": "22.13.1",
    "@types/nunjucks": "3.2.6",
    "@typescript-eslint/eslint-plugin": "8.23.0",
    "@typescript-eslint/parser": "8.23.0",
    "eslint": "9.19.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-n": "^14.0.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-unicorn": "56.0.1",
    "husky": "9.1.7",
    "prettier": "3.4.2",
    "shipjs": "0.27.0",
    "sort-package-json": "2.14.0",
    "typescript": "5.7.3",
    "vite": "6.1.0"
  },
  "peerDependencies": {
    "@pulumi/aws": "^5.0 || ^6.0.0",
    "@pulumi/cloudflare": "^5.8",
    "@pulumi/command": "^0.7 || ^0.8.0 || ^0.9.0 || ^1.0.0",
    "@pulumi/digitalocean": "^4.20",
    "@pulumi/gitlab": "^6.1 || ^8.0.0",
    "@pulumi/pulumi": "^3.0"
  },
  "engines": {
    "node": ">=16"
  }
}
